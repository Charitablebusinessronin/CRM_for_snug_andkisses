{
  "name": "snug-kisses-crm",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "dev:fe:3005": "next dev -p 3005 --turbo",
    "dev:be:3006:win": "set PORT=3006&& npm run ts:dev",
    "dev:be:3006:nix": "PORT=3006 npm run ts:dev",
    "dev:3005-3006": "concurrently -k -n FE,BE -c green,blue \"npm run dev:fe:3005\" \"npm run dev:be:3006:win\"",
    "lint": "next lint",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true next build",
    "clean": "rimraf .next out dist",
    "test:e2e": "playwright test",
    "test:e2e:docker": "docker compose run --rm playwright",
    "ts:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "ts:build": "tsc -p tsconfig.server.json",
    "ts:start": "node dist/server.js",
    "server": "npm run ts:dev",
    "start:express": "node server.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:rebuild": "docker compose down && docker compose up --build -d",
    "docker:logs": "docker compose logs -f",
    "docker:health": "curl -f http://localhost:5369/api/health && curl -f http://localhost:4728/health"
  },
